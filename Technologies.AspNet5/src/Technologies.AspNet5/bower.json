{
  "name": "bower",
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "jquery": "2.1.4",
    "bootstrap": "3.3.5",
    "angular": "1.4.2",
    "angular-bootstrap": "0.13.0",
    "angular-ui": "0.4.0",
    "font-awesome": "*"
  },
  "exportsOverride": {
    "bootstrap": {
      "js": "dist/js/*.*",
      "css": "dist/css/*.*",
      "fonts": "dist/fonts/*.*"
    },
    "jquery": {
      "": "dist/*.*"
    },
    "angular": {
      "": "*.{js,css,map,gzip}"
    },
    "angular-bootstrap": {
      "": "*.*"
    },
    "angular-ui": {
      "": "build/*.{js,css}"
    },
    "font-awesome": {
      "css": "css/*.*",
      "fonts": "fonts/*.*"
    }
  }
}
